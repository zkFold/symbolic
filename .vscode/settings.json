{
  "haskell.manageHLS": "GHCup",
  "haskell.toolchain": {
    "hls": "2.10.0.0",
    "cabal": "3.14.2.0",
    "ghc": "9.6.7",
    "stack": null
  },
  "haskell.formattingProvider": "none",
  "[haskell]": {
    "editor.formatOnSave": false
  },
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "\\.hs$",
        "cmd": "fourmolu --mode inplace ${file}"
      }
    ]
  }
}